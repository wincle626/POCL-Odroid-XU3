all: ${KERNEL_BC}

vpath %.c ../../../../lib/kernel/hsail64 ../../../../lib/kernel
vpath %.cc ../../../../lib/kernel/hsail64 ../../../../lib/kernel
vpath %.cl ../../../../lib/kernel/hsail64 ../../../../lib/kernel
vpath %.ll ../../../../lib/kernel/hsail64 ../../../../lib/kernel

# Generate a precompiled header for the built-in function
# declarations, in case supported by the target.

# Note: the precompiled header must be compiled with the same features
# as the kernels will be. That is, use exactly the same frontend
# feature switches. Otherwise it will fail when compiling the kernel
# against the precompiled header.
_kernel.h.pch: ../../../include/${TARGET_DIR}/types.h ../../../../include/_kernel.h
	/root/enpower/llvm/llvm_37/install/bin/clang -Xclang -ffake-address-space-map -fno-math-errno -fblocks -fno-builtin -fasm -Wno-format  -Xclang -cl-std=CL2.0 -D__OPENCL_C_VERSION__=200 -Xclang -ffake-address-space-map -c -target ${KERNEL_TARGET} -x cl \
	-include ../../../include/${TARGET_DIR}/types.h \
	-Xclang -emit-pch ../../../../include/_kernel.h -o _kernel.h.pch 

# Rules to compile the different kernel library source file types into
# LLVM bitcode
%.c.bc: %.c ${abs_top_srcdir}/include/pocl_types.h ${abs_top_srcdir}/include/_kernel_c.h ${LKERNEL_HDRS_EXTRA}
	mkdir -p ${dir $@}
	/root/enpower/llvm/llvm_37/install/bin/clang ${CLANG_FLAGS} ${CLFLAGS} ${DEVICE_CL_FLAGS} -D__CBUILD__ -c -o $@ -include ${abs_top_srcdir}/include/_kernel_c.h $<
%.cc.bc: %.cc  ${LKERNEL_HDRS_EXTRA}
	mkdir -p ${dir $@}
	/root/enpower/llvm/llvm_37/install/bin/clang++ ${CLANG_FLAGS} ${CLANGXX_FLAGS} ${DEVICE_CL_FLAGS} -c -o $@ $<
%.cl.bc: %.cl ${abs_top_srcdir}/include/_kernel.h ${abs_top_srcdir}/include/_kernel_c.h ${abs_top_srcdir}/include/pocl_types.h ${LKERNEL_HDRS_EXTRA}
	mkdir -p ${dir $@}
	/root/enpower/llvm/llvm_37/install/bin/clang ${CLANG_FLAGS} -x cl ${CLFLAGS} ${DEVICE_CL_FLAGS} -fsigned-char -c -o $@ $< -include ${abs_top_srcdir}/include/_kernel.h
%.ll.bc: %.ll
	mkdir -p ${dir $@}
	/root/enpower/llvm/llvm_37/install/bin/llvm-as -o $@ $<

# Optimize the bitcode library to speed up optimization times for the
# OpenCL kernels
${KERNEL_BC}: ${OBJ}
	/root/enpower/llvm/llvm_37/install/bin/llvm-link $^ -o - | /root/enpower/llvm/llvm_37/install/bin/opt ${LLC_FLAGS} ${KERNEL_LIB_OPT_FLAGS} -O3 -fp-contract=off -o $@

